public class Stringpalindrom {

	public static void main(String[] args) {
		String s="lol";
		String rev="";
		for(int i=s.length()-1;i>=0;i--) 
			rev=rev+s.charAt(i);
			if(s.equals(rev)) 
				System.out.println("string is a palindrome");
			
			else 
				System.out.println("string is not a palindrome");
			
			
		
		
	}

}
